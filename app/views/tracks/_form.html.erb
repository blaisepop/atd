<%= simple_form_for ([@playlist,@track]) do |f| %>
  <p><%= f.label :title %></p>
  <p><%= f.text_field :title %></p>
  <p><%= f.label :artist %></p>
  <p><%= f.text_field :artist %></p>
  <p><%= f.label :album %></p>
  <p><%= f.text_field :album %></p>
  <p><%= f.label :duration %></p>
  <p><%= f.input :duration, as: :decimal %></p>
  <p><%= f.submit %>
<% end %>
